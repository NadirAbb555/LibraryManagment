            <i class="visit"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.deliveryBook</h3>
            <p>Delivery</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="clearfix"></div>
</div>

