@model MVCLibaryApp.Models.Entity.TBLPERSONAL
@{
    ViewBag.Title = "ChangePersonal";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("UpdatePersonal", "Personal", FormMethod.Post))
{
    @Html.HiddenFor(m => m.ID)
    <label>Personal</label>
    @Html.TextBoxFor(m => m.PERSONAL, new { @class = "form-control" })
    @Html.ValidationMessageFor(m => m.PERSONAL,"", new { @style = "color:red" })
    <br />
    <button class="btn btn-primary">Update</button>
}
